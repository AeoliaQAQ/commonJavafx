<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<!-- 弹窗 -->

<AnchorPane fx:id="rootPane" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="title" layoutX="78.0" layoutY="-3.0" prefHeight="33.0" prefWidth="40.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutY="30.0" prefHeight="510.0" prefWidth="401.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
      <Label layoutY="542.0" text="记录总计:" />
      <Label fx:id="totalCount" layoutX="54.0" layoutY="542.0" text="0" />
      <Button layoutX="181.0" layoutY="550.0" mnemonicParsing="false" onAction="#doConfirm" text="确定" />
      <Button layoutX="244.0" layoutY="550.0" mnemonicParsing="false" onAction="#cancelAction" text="取消" />
      <Label layoutX="36.0" layoutY="-3.0" prefHeight="33.0" prefWidth="40.0" text="浏览">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
